{
    "name":  "progresivjose/laravel-piano-oauth",
    "description": "A laravel wrapper to progresivjose/piano-oauth package",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.15.0",
        "illuminate/http": "^10.15.0",
        "progresivjose/piano-oauth": "^1.0",
        "illuminate/database": "^10.15.0"
    },
    "autoload": {
        "psr-4": {
            "Progresivjose\\LaravelPianoOauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "José Manuel González Ricart",
            "email": "jmanuelgricart@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
